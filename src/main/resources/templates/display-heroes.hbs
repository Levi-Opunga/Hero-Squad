{{#partial "content"}}
    <div class="text-center col-md-10">
    {{#if squads}}
            <h2 class="display-2">Available Heroes</h2><
        {{#each squads}}
            <h2>{{name}}</h2>
            <h3>Motto</h3>
            <p>{{motto}}</p>
            <h3>Cause</h3>
            <p>Squad :{{Cause}}</p>

            <h4>Squad Capacity :{{max-size}}</h4>
        {{/each}} <a href="/squad-form">
            <button class="btn btn-success">Add More Heroes</button>
        </a>
    {{/if}}
    /div>

{{/partial}}partial}}
{{>layout.hbs}}